{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "ordered-imports": [
            true,
            {
                "import-sources-order": "any",
                "named-imports-order": "any"
            }
        ],
        "member-access": [
            true,
            "no-public"
        ],
        "max-line-length": [
            true,
            500
        ],
        "no-shadowed-variable": false,
        "object-literal-sort-keys": false,
        "trailing-comma": [
            true,
            {
                "singleline": "never",
                "multiline": {
                    "objects": "ignore",
                    "arrays": "ignore",
                    "functions": "never",
                    "typeLiterals": "ignore"
                }
            }
        ],
        "no-var-requires": false
    },
    "rulesDirectory": []
}